<?xml version="1.0"?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../doc/drakeURDF.xsd" name="PushingArm">

  <link name="dummy_base_link">
    <inertial>
        <mass value="0"/>
        <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
  </link>

  <joint name="dummy_joint" type="fixed">
    <parent link="dummy_base_link"/>
    <child link="base_link"/>
    <origin rpy="3.1415 0 0" xyz="0 0 6"/>
  </joint>

  <link name="base_link">
    <inertial>
        <mass value="1"/>
        <inertia ixx="1"  ixy="1"  ixz="1" iyy="1" iyz="1" izz="1" />
    </inertial>
    <visual>
      <geometry>
        <cylinder length="4" radius="0.2"/>
      </geometry>
      <material name="c1">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="ball">
    <inertial>
        <mass value="1"/>
        <!-- <inertia ixx="1"  ixy="1"  ixz="1" iyy="1" iyz="1" izz="1" /> -->
        <inertia ixx="0"  ixy="0"  ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
    <visual>
      <origin xyz="0.4 0 8" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0.5"/>
      </geometry>
      <material name="c2">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.4 0 8" rpy="0 0 0"/>
      <geometry>
        <sphere radius="0"/>
        <!-- <sphere radius="0.5"/> -->
      </geometry>
    </collision>
  </link>

  <link name="link_1">
    <inertial>
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 1.5" rpy="0 0 0"/>
      <geometry>
        <cylinder length="3.0" radius="0.2"/>
      </geometry>
      <material name="c2">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="link_2">
    <inertial>
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 1.5" rpy="0 0 0"/>
      <geometry>
        <cylinder length="3.0" radius="0.2"/>
      </geometry>
      <material name="c3">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="link_3">
    <visual>
      <origin xyz="0 0 1.5" rpy="0 0 0"/>
      <geometry>
        <cylinder length="3.0" radius="0.2"/>
      </geometry>
      <material name="c3">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.5"/>
      <geometry>
        <sphere radius="0"/>
      </geometry>
    </collision>

    <!-- <collision> -->
    <!--   <origin xyz="0 0 1.5"/> -->
    <!--   <geometry> -->
    <!--     <cylinder length="3.0" radius="0.2"/> -->
    <!--   </geometry> -->
    <!-- </collision> -->
  </link>
  <link name="link_mass">
    <inertial>
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 1.5" rpy="0 0 0"/>
      <geometry>
        <cylinder length="3.0" radius="0.2"/>
      </geometry>
      <material name="c3">
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>

    <!-- <collision> -->
    <!--   <origin xyz="0 0 1.5"/> -->
    <!--   <geometry> -->
    <!--     <cylinder length="3.0" radius="0.2"/> -->
    <!--   </geometry> -->
    <!-- </collision> -->
  </link>
  <joint name="left_lower_leg_mass_weld" type="fixed">
    <parent link="link_3"/>
    <child link="link_mass"/>
  </joint>



  <joint name="joint_0" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin rpy="0 0 0" xyz="0 0 2"/>    
  </joint>

  <joint name="joint_1" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin rpy="0 0 0" xyz="0 0 3"/>
    <limit effort="100" velocity="100"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>
  
  <joint name="joint_2" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin rpy="0 0 0" xyz="0 0 3"/>
    <limit effort="100" velocity="100"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <joint name="joint_b" type="prismatic">
    <axis xyz="1 0 0"/>
    <parent link="base_link"/>
    <child link="ball"/>
    <origin rpy="0 0 0" xyz="2 0 0"/>    
    <limit effort="100" velocity="100" lower="-30" upper="30"/>
  </joint>


  <transmission name="base_trans" type="SimpleTransmission">
    <actuator name="base_torque"/>
    <joint name="joint_0"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="joint_1_trans" type="SimpleTransmission">
    <actuator name="joint_1_torque"/>
    <joint name="joint_1"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>
  <transmission name="joint_2_trans" type="SimpleTransmission">
    <actuator name="joint_2_torque"/>
    <joint name="joint_2"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission>

</robot>
